{
    "version": 2,
    "builds": [
      {
        "src": "client/build/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "server/index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server/$1" },
      { "src": "/(.*)", "dest": "/client/build/$1" }
    ]
  }